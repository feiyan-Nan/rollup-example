{
  "presets": [
    [
      "@babel/env",
      {
        "modules":false,
        "targets": "> 0.25%, not dead",
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    ["@babel/preset-react"]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3
      }
    ]
  ],
  "ignore": [
    "node_modules/**"
  ]
}
